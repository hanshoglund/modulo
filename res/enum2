typedef enum _baz { x, y, z = 13 } baz;

CDecl 
    [
        CStorageSpec typedef,
        CTypeSpec 
            (CEnumType 
                (CEnum 
                    (Just `_baz') 
                    (Just 
                        [(`x',Nothing),
                         (`y',Nothing),
                         (`z',Just (CConst (CIntConst 13  )))]
                    )[]))
    ] 
    [
        (Just (CDeclr (Just `baz') [] Nothing []  ),Nothing,Nothing)
    ]